model IEnumerable<SafeEats.Models.Recipe>
    @{
        Layout = "~/Views/Shared/_SafeEatsLayout.cshtml";
    }
@{
    ViewBag.Title = "Profile";
}
<h2>@ViewBag.Title</h2>
    <body ng-app="SafeEats">
        @Html.Partial("_NavPartial")        
            <h1>hi</h1>
            <div class="container-fluid">
                <div class="row" style="display:inline-flex;">
                    @foreach (var item in Model)
                    {
                        <div>
                            <div>@item.Title</div>
                            <div>
                                <ul class="center"></ul>
                            </div>
                            <div ng-controller="ProfileController">
                                <div style="display:none" id=@item.RecipeId></div>                                
                            </div>
                        </div>
                    }
                </div>
            </div>
    </body>
    
